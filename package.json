  "description": "",
  "main": "server.js",
  "scripts": {
    "copy": "cp server.js build/ && cp -r public/ build/public/",
    "clean": "rm -rf build && mkdir build",
    "build": "NODE_ENV=production npm run build-server && npm run build-react",
    "prebuild": "npm run clean && npm run copy",
    "build-server": "babel ./backend -d build/backend",
    "build-react": "webpack -p --config webpack.config.prod.js",
    "dev": "concurrently -k \"webpack-dev-server --hot\" \"npm run backend\"",
    "frontend": "webpack-dev-server --hot",
    "backend": "nodemon server.js --exec babel-node --presets @babel/env",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles",
    "webpack": "webpack --config webpack.config.prod.js"
  },
